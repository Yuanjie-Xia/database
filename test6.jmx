    	   	vars.put(&quot;id&quot;+i,group_timestamp[3]);
    	   	if(i&gt;1){
    	   	long duration = prev.getTime();}
    	   	//log.info(&quot;time1: {}&quot;,String.valueOf(duration));}
    	   	//pageDelay=prev.getParent().getTime()
    	     //log.info(&quot;pageDelay: {}&quot;,pageDelay)
    	   	
    	   	
    	   }
        i++;
    }
     bufRdr.close();            
}
catch (Exception ex) {
    IsSuccess = false; 
    log.error(ex.getMessage());
}
catch (Throwable thex) {
    log.error(thex.getMessage());
}
</stringProp>
